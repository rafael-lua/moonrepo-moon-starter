{
  "name": "moonrepo-sst-starter",
  "packageManager": "yarn@4.10.3",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "author": "rafael-lua",
  "scripts": {
    "create:pkg": "mkdir -p packages/$1 && cd packages/$1 && yarn init -y",
    "create:mdl": "mkdir -p modules/$1 && cd modules/$1 && yarn init -y",
    "moon": "$(yarn bin moon) --color",
    "graph": "yarn moon tg",
    "postinstall": "yarn moon sync projects",
    "test:ui": "vitest --coverage --ui --api.port 3005",
    "format": "prettier --write ."
  },
  "workspaces": [
    "packages/*",
    "modules/*"
  ],
  "devDependencies": {
    "@moonrepo/cli": "^1.41.6",
    "@prettier/plugin-oxc": "^0.0.4",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "changelogen": "^0.6.2",
    "oxlint": "^1.18.0",
    "oxlint-tsgolint": "^0.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite-plugin-oxlint": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  },
  "engines": {
    "node": ">=22"
  }
}
